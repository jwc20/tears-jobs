<div class="center">
  <h1>Tech. Entry and Remote Jobs</h1>
  <table>
    <thead>
      <tr>
        <th>Company Name</th>
        <th>Job Title</th>
        <th>Job Link</th>
      </tr>
    </thead>
    <tbody>
      <% @job_listings.each do |job_listing| %>
      <tr>
        <td><%= job_listing.company_name %></td>
        <td><%= job_listing.job_title %></td>
        <td class="link">
          <%= link_to "Link", job_listing.job_link %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <div class="pagination"><%= will_paginate @job_listings %></div>
</div>